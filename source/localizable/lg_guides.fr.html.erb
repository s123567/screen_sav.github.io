<div class="container">
  <div class="logo_container">
    <%= image_tag "logo_webos.png", class:"os_logo" %>
  </div>
  <div class="webos-versions">
    <div class="card" style="width: 18rem;">
    <div class="card-body lang-choice">
      <a href="#" class="btn btn-secondary">FR</a>
      <a href="/en/lg_guides.html" class="btn btn-secondary">EN</a>
    </div>
  </div>
    <% data.lg_guides.each do |g| %>
      <div class="version">
        <div class="card" style="width: 18rem;">
          <div class="card-body os">
            <%= link_to "#{g.version}", "#{g.link}.html", class:"stretched-link" %>
          </div>
        </div>
      </div>
    <% end %>
    <%= partial("partials/os_search", :locals => { :brand => :lg }) %>
    <div class="back-menu">
      <a class="" href="../index.html"><%= partial 'images/icons/arrow-left-circle' %> </a>
    </div>
  </div>
</div>
